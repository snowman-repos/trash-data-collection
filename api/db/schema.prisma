datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Record {
  id                  Int     @id @default(autoincrement())
  date                String
  group               String
  location            String
  numberOfVolunteers  Int
  totalWeight         Float
  trashBagsUsed       Int
  cans                Int
  drums               Int
  glass               Int
  electronics         Int
  footwear            Int
  jerryCans           Int
  plasticContainers   Int
  plasticStraws       Int
  smokingRelated      Int
  tires               Int
  other               String
}

model TrashData {
  id    Int @id @default(autoincrement())
  data  String
}